{
  "version": 3,
  "console_title_template": "{{ .Folder }}",
  "enable_cursor_positioning": true,
  "final_space": true,
  "shell_integration": true,
  "var": {
    "red": "#C53B53",
    "clock": "",
    "ok": ""
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#3B4261",
    "template": "{{ .Shell }}> "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "force": true,
      "segments": [
        {
          "type": "os",
          "background": "#3B4261",
          "foreground": "#C0CAF5",
          "leading_diamond": "╭─",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "properties": {
            "macos": " "
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "background": "#7AA2F7",
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "template": " \uf07c {{ .Path }} "
        },
        {
          "type": "git",
          "background": "#4e9a06",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "force": true,
      "segments": [
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "type": "node"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "type": "go"
        },
        {
          "background": "#FF6F61",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue7c9 ",
          "type": "php"
        },
        {
          "background": "#FFDE57",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "type": "python"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7",
          "type": "azfunc"
        },
        {
          "type": "root",
          "style": "powerline",
          "background": "#ffff66",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": "   "
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "background": "#FABA4A",
          "foreground": "#000000",
          "invert_powerline": true,
          "powerline_symbol": "",
          "template": " {{ .FormattedMs }}  "
        },
        {
          "type": "status",
          "style": "powerline",
          "background": "#9ECE6A",
          "background_templates": [
            "{{ if gt .Code 0 }}{{ .Var.red }}{{ end }}"
          ],
          "foreground": "#000000",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ffffff{{ end }}"
          ],
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true
          },
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}{{ .Var.ok }}{{ end }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "background": "#292E42",
          "foreground": "#7AA2F7",
          "invert_powerline": true,
          "template": " {{ .CurrentDate | date .Format }} {{ .Var.clock }} ",
          "trailing_diamond": "─╮"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "force": true,
      "newline": true,
      "segments": [
        {
          "foreground": "#3B4261",
          "style": "plain",
          "template": "╰─",
          "type": "text"
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "force": true,
      "segments": [
        {
          "foreground": "#3B4261",
          "style": "plain",
          "template": "─╯",
          "type": "text"
        }
      ]
    }
  ]
}